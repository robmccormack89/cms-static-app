<!DOCTYPE html>
<html lang="{{config.language}}">

  <head>
    
    <meta charset="{{config.charset}}">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset={{config.charset}}" />
    
    {% block google_fonts %}
      <link rel="preconnect" href="https://fonts.googleapis.com">
      <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
      <!-- <link href="https://fonts.googleapis.com/css2?family=Playfair+Display:wght@900&family=Raleway:wght@400;500;700&display=swap" rel="stylesheet"> -->
      <!-- <link href="https://fonts.googleapis.com/css2?family=Playfair+Display:wght@900&family=Quicksand:wght@400;500;600;700&display=swap" rel="stylesheet"> -->
      <link href="https://fonts.googleapis.com/css2?family=Montserrat:wght@500;600;700;800&family=Playfair+Display:wght@900&family=Work+Sans&display=swap" rel="stylesheet">
    {% endblock %}
    
    {% block base_seo_meta %}
      <!-- WebSite -->
      <script type="application/ld+json">
        {
          "@context": "https://schema.org/",
          "@type": "WebSite",
          "name": "{{config.site_title}}",
          "url": "{{config.base_url}}",
          "potentialAction": {
            "@type": "SearchAction",
            "target": "{{config.base_url}}/?s={search_term_string}",
            "query-input": "required name=search_term_string"
          }
        }
      </script>
      <!-- Person -->
      <script type="application/ld+json">
        {
          "@context": "https://schema.org/",
          "@type": "Person",
          "name": "{{config.name}}",
          "url": "{{config.base_url}}",
          "image": "{{config.avatar_src}}",
          "sameAs": [
            "{{config.github_url}}",
            "{{config.base_url}}"
          ],
          "jobTitle": "{{config.job}}",
          "worksFor": {
            "@type": "Organization",
            "name": "{{config.name}}"
          }  
        }
      </script>
    {% endblock %}
    
    {% block seo_meta %}
    {% endblock %}
    
    {% block base_styles %}
      <!-- base uikit scss build -->
      <link rel="stylesheet" type="text/css" href="/public/css/base.css">
      <!-- font awesome scss build -->
      <link rel="stylesheet" type="text/css" href="/public/css/global.css">
      <!-- base uikit scripts: no icons -->
      <script src="/public/js/global/base.min.js"></script>
    {% endblock %}
    
    {% block styles %}
      <style>
      
        /* 
        
        GLOBAL: Header
        
        */
    
        /* header layered position */
        .site-header {
          z-index: 2;
        }
        
        /* header third level dropdown */
        .site-header .third-drop {
          left: 140px!important;
          top: 30px!important;
        }
        
        /* header navbar items text */
        .site-header .uk-navbar-nav>li>a {
          text-transform: capitalize;
        }
        
        /* header navbar items font size */
        .site-header .uk-navbar-item, 
        .site-header .uk-navbar-nav>li>a, 
        .site-header .uk-navbar-toggle {
          font-size: 1rem;
        }
        
        /* header logo subtext positioning */
        .site-header .by-rmcc-text {
          position: absolute;
          bottom: -8px;
          width: max-content;
          font-size: .75rem;
          left: 15px;
        }
        .site-header .uk-sticky-below .by-rmcc-text {
          bottom: 0;
          font-size: .7rem;
        }
        
        /* header & logo padding & margins */
        .site-header nav {
          padding-bottom: 20px;
          padding-top: 20px;
          margin-top: -9px;
        }
        .site-header .uk-sticky-below nav {
          padding-bottom: 0;
          padding-top: 10px;
        }
        .site-header .main-menu-logo .logo-item {
          margin-top: -10px;
        }
        .site-header .uk-sticky-below .main-menu-logo .logo-item {
          margin-top: -5px;
        }
        
        /* header dark/light colors */
        .site-header .uk-background-default .uk-navbar-item, 
        .site-header .uk-background-default .uk-navbar-nav>li>a,
        .site-header .uk-background-default .uk-navbar-nav>li:hover>a, 
        .site-header .uk-background-default .uk-navbar-nav>li>a.uk-open, 
        .site-header .uk-background-default .uk-navbar-nav>li>a:focus,
        .site-header .uk-navbar-transparent .uk-navbar-item, 
        .site-header .uk-navbar-transparent .uk-navbar-nav>li>a,
        .site-header .uk-navbar-transparent .uk-navbar-nav>li:hover>a, 
        .site-header .uk-navbar-transparent .uk-navbar-nav>li>a.uk-open, 
        .site-header .uk-navbar-transparent .uk-navbar-nav>li>a:focus {
          color: #1a191d;
        }
        .uk-light .site-header .uk-background-default .uk-navbar-item, 
        .uk-light .site-header .uk-background-default .uk-navbar-nav>li>a,
        .uk-light .site-header .uk-background-default .uk-navbar-nav>li:hover>a, 
        .uk-light .site-header .uk-background-default .uk-navbar-nav>li>a.uk-open, 
        .uk-light .site-header .uk-background-default .uk-navbar-nav>li>a:focus,
        .uk-light .site-header .uk-navbar-transparent .uk-navbar-item, 
        .uk-light .site-header .uk-navbar-transparent .uk-navbar-nav>li>a,
        .uk-light .site-header .uk-navbar-transparent .uk-navbar-nav>li:hover>a, 
        .uk-light .site-header .uk-navbar-transparent .uk-navbar-nav>li>a.uk-open, 
        .uk-light .site-header .uk-navbar-transparent .uk-navbar-nav>li>a:focus ,
        .uk-light.uk-navbar-transparent .uk-navbar-item, 
        .uk-light.uk-navbar-transparent .uk-navbar-nav>li>a,
        .uk-light.uk-navbar-transparent .uk-navbar-nav>li:hover>a, 
        .uk-light.uk-navbar-transparent .uk-navbar-nav>li>a.uk-open, 
        .uk-light.uk-navbar-transparent .uk-navbar-nav>li>a:focus {
          color: #fff!important;
        }
        
        /* 
        
        GLOBAL: Backgrounds
        
        */
        
        .uk-background-secondary {
          background-color: #17161a;
        }
        
        /* custom class: uk-background-pink */
        .uk-background-pink {
          background-color: #ffecf3;
        }
        .uk-light .uk-background-pink {
          background-color: #17161a;
        }
        
        /* custom class: uk-background-muted-light */
        .uk-background-muted-light {
          background-color: #f9f9f9;
        }
        /* custom class: uk-background-muted-light: input & select inputs */
        .uk-background-muted-light .uk-input, .uk-background-muted-light .uk-select {
          background-color: #f0f0f0;
        }
        .uk-background-muted-light .uk-input, .uk-background-muted-light .uk-select {
          background-color: #f0f0f0;
        }
        .uk-light .uk-background-muted-light {
          background-color: #201e25;
        }
        .uk-light .uk-background-muted-light .uk-input, 
        .uk-light .uk-background-muted-light .uk-select {
          background-color: #17161a;
        }
        
        /* 
        
        GLOBAL: Containers
        
        */
      
        .uk-container {
          /* max-width: 1300px; */
          max-width: 1250px;
        }
        .uk-container-medium {
          max-width: 1100px;
        }
        .uk-container-large {
          max-width: 1400px;
        }
        .uk-container-small {
          max-width: 900px;
        }
        .uk-container-expand {
          max-width: none;
        }
        
        /* 
        
        GLOBAL: Grids
        
        */
        
        .uk-grid-collapse>*, .uk-grid-column-collapse>* {
          padding-left: 0;
        }
        
        
        /* 
        
        GLOBAL: Buttons
        
        */
        
        .uk-button {
          font-weight: 600;
        }
        .uk-background-primary {
          background-color: #d90a2c;
        }
        .uk-button-secondary {
          background-color: #17161a;
        }
        .uk-button-secondary:focus, .uk-button-secondary:hover,
        .uk-light .uk-button-secondary:focus, .uk-light .uk-button-secondary:hover {
          background-color: #d90a2c;
          color: #fff;
        }
        .uk-light .uk-button-secondary {
          background-color: #fff;
          color: #1a191d;
        }
        .uk-button-text:before {
          border-width: 2px;
        }
        .uk-button-small {
          font-size: 1rem;
          line-height: 34px;
          padding: 0px 15px;
        }
        
        /* add to links with uk-text-capitalize uk-animation-toggle */
        .style-btn {
          color: #17161a;
          transition: .2s ease-in-out;
          transition-property: color;
        }
        .style-btn:hover, .style-btn:focus {
          color: #d90a2c;
          text-decoration: none;
        }
        .uk-light .style-btn {
          color: #fff;
        }
        .uk-light .style-btn:hover {
          color: #d90a2c;
        }
        
        /* 
        
        GLOBAL: More uikit
        
        */
        
        .uk-offcanvas-bar {
          width: 80vw;
        }
        a, .uk-link {
          color: #d90a2c;
        }
        
        /* 
        
        GLOBAL: Fonts & Text
        
        */
        
        html {
          color: #17161a;
          /* font-family: -apple-system,BlinkMacSystemFont,Roboto,"Segoe UI",Helvetica,Arial,sans-serif,"Apple Color Emoji","Segoe UI Emoji","Segoe UI Symbol"; */
          /* font-family: 'Quicksand', sans-serif; */
          font-family: 'Work Sans', sans-serif;
          /* font-weight: 500; */
          text-rendering: optimizeLegibility;
          -webkit-font-smoothing: antialiased;
          -moz-osx-font-smoothing: grayscale;
        }
        .uk-navbar-item, .uk-navbar-nav>li>a, .uk-navbar-toggle {
          /* font-family: 'Raleway', sans-serif; */
          /* font-family: 'Quicksand', sans-serif; */
          font-family: 'Montserrat', sans-serif;
          font-weight: 400;
        }
        .uk-h1, .uk-h2, .uk-h3, .uk-h4, .uk-h5, .uk-h6, .uk-heading-2xlarge, .uk-heading-large, .uk-heading-medium, .uk-heading-small, .uk-heading-xlarge, 
        h1, h2, h3, h4, h5, h6 {
          /* font-family: 'Raleway', sans-serif; */
          /* font-family: 'Quicksand', sans-serif; */
          font-family: 'Montserrat', sans-serif;
          font-weight: 700;
          color: #17161a;
        }
        .uk-h1, .uk-h2, .uk-h3, .uk-h4, .uk-h5, .uk-h6, .uk-heading-small, 
        h1, h2, h3, h4, h5, h6 {
          letter-spacing: -1px;
        }
        .uk-heading-2xlarge, .uk-heading-large, .uk-heading-medium, .uk-heading-xlarge {
          letter-spacing: -3px;
        }
        .uk-text-bold,
        strong {
          font-weight: 700;
        }
        
        /* site logo */
        .site-logo, 
        .site-header .site-logo {
          /* font-family: 'Raleway', sans-serif; */
          /* font-family: 'Quicksand', sans-serif; */
          font-family: 'Montserrat', sans-serif;
          text-decoration: none!important;
          font-weight: 700;
          letter-spacing: -1px;
          font-size: 1.5rem;
          line-height: 20px;
        }
        .site-header .uk-sticky-below .site-logo {
          font-size: 1.2rem;
          line-height: 16px;
        }
        .by-rmcc-text {
          /* font-family: 'Raleway', sans-serif; */
          /* font-family: 'Quicksand', sans-serif; */
          font-family: 'Montserrat', sans-serif;
          font-weight: 400;
        }
        
        /* 
        
        HOME: #PersonalLeader
        
        */
        
        #PersonalLeader {
          overflow: hidden;
        }
        #PersonalLeader .leader-height {
          min-height: calc(100vh);
        }
        #PersonalLeader .bg-text, #PersonalLeader .bg-text > * {
          font-family: 'Playfair Display';
          font-size: 600px;
          font-weight: 900;
          color: rgba(23,22,26,0.08);
          line-height: .8!important;
        }
        #PersonalLeader .bg-text {
          transform: rotate(-30deg) translateY(-50%);
          position: absolute;
          top: -70%;
          left: -100%;
          z-index: -1;
        }
        #PersonalLeader .bg-text p {
          line-height: 75%;
          white-space: nowrap;
          position: absolute;
          z-index: -1;
        }
        #PersonalLeader .uk-text-meta {
          font-size: 0.9rem;
          color: #ffffffc7!important;
        }
        
        /* 
        
        HOME: #Services
        
        */
        
        #Services .uk-grid {
          margin-top:-225px;
        }
        #Services .uk-overlay-primary {
          background: rgb(34 34 34 / 46%);
        }
        
        /* 
        
        GLOBAL: Img hover effects
        
        */
          
        /* hover image colour effects */
        .hover-colour-wrap {
          height:100%;
          width:100%
        }
        .hover-colour-wrap .uk-heading-bullet:before {
          border-width: 4px;
          transition: .5s border-color linear;
        }   
        .hover-colour-wrap:hover .uk-heading-bullet:before {
          border-color: #d90a2c!important;
        }      
        .hover-colour-wrap img {
          height:100%;
          width:100%;
          object-fit:cover;
          filter:grayscale(1);
          transition: .5s filter linear;
        }
        .hover-colour-wrap:hover img {
          filter:grayscale(0);
        }
        
        /* hover image grow effects */
        .hover-grow-wrap {
          overflow: hidden;
        }
        .hover-grow-wrap img {
          transition: .2s transform linear;
        }
        .hover-grow-wrap:hover img {
          transform: scale(1.1);
        }
        
        /* 
        
        GLOBAL: Img fixing to a box
        
        */
        
        .object-fitting {
          height:100%;
          width:100%;
          object-fit:cover;
        }
        .object-fit-cover {
          object-fit:cover;
        }
        
        
        /* 
        
        HOME: #CTA
        
        */
        
        #CTA .uk-accordion-title:focus, .uk-accordion-title:hover {
          color: #fff;
        }
        #CTA .uk-accordion-title:before {
          float: left;
          margin-left: 0;
          margin-right: 10px;
        }
        
        /* 
        
        #FooterTop
        
        */
        
        #FooterTop *+.uk-grid-margin-medium, #FooterTop .uk-grid+.uk-grid-medium, #FooterTop .uk-grid+.uk-grid-row-medium, #FooterTop .uk-grid-medium>.uk-grid-margin, #FooterTop .uk-grid-row-medium>.uk-grid-margin {
          margin-top: 20px;
        }
        #FooterTop .uk-grid-column-small>*, #FooterTop .uk-grid-small>* {
          padding-left: 20px;
        }
        
        /* 
        
        Inputs
        
        */
        
        /* input & select inputs */
        .uk-input, .uk-select, .uk-textarea {
          background-color: #f8f8f8;
          border: none;
          transition: .1s ease-in-out;
          transition-property: background-color,border-color;
        }
        
        /* input & select inputs: hover & focus */
        .uk-input:focus, .uk-input:hover, 
        .uk-select:focus, .uk-select:hover,
        .uk-textarea:focus, .uk-textarea:hover {
          background-color: #f3f3f3;
        }
        
        /* input & select inputs: uk-light */
        .uk-light .uk-input, 
        .uk-light .uk-select,
        .uk-light .uk-textarea {
          background-color: rgba(255,255,255,.05);
        }
        
        /* uk-form-large size */
        .uk-form-large:not(textarea):not([multiple]):not([size]) {
          height: 45px;
          padding-left: 12px;
          padding-right: 12px;
        }
        .uk-form-large {
          font-size: 1rem;
        }
        
        /* placeholder text color */
        ::placeholder {
          color: #777777!important;
          opacity: 1!important;
        }
        :-ms-input-placeholder {
          color: #777777;
        }
        ::-ms-input-placeholder {
          color: #777777;
        }
        
        /* search inputs */
        .uk-search-default .uk-search-input {
          background-color: #f8f8f8;
          border: none;
          transition: .1s ease-in-out;
          transition-property: background-color,border-color;
        }
        .uk-search-default .uk-search-input:focus, .uk-search-default .uk-search-input:hover {
          background-color: #f3f3f3;
        }
        .uk-light .uk-search-default .uk-search-input {
          background-color: rgba(255,255,255,.05);
        }
        .uk-search .uk-search-icon {
          color: #17161a;
        }
        .uk-search .uk-form-large:not(textarea):not([multiple]):not([size]) {
          height: 50px;
          padding-left: 15px;
          padding-right: 35px;
        }
        
        /* hide data list dropdown icon for autopopulated suggestions inputs */
        input::-webkit-calendar-picker-indicator {
          display: none!important;
        }
        [list]::-webkit-calendar-picker-indicator {
          display: none!important;
        }
        
        /* 
        
        the single post tease post
        
        */
        
        .post-details {
          font-size: .95rem;
        }
        .uk-light .post-details {
          color: #fff;
        }
        .taxonomy-holder:not(.no-divider)::after {
          content: "•";
          display: inline-block;
          vertical-align: middle;
          font-size: 0.8em;
          margin-top: -2px;
          color: #d90a2c;
        }
        .taxonomy-holder .taxonomy {
          font-weight: 600;
          /* font-family: 'Montserrat'; */
        }
        .post-headline {
          line-height: 1.3;
          font-weight: 700;
          letter-spacing: -.01em;
        }
        .post-headline a {
          transition: .2s ease-in-out;
          transition-property: color;
        }
        .post-headline a:hover {
          color: #d90a2c;
          text-decoration: none;
        }
        blockquote {
          color: #333;
          font-size: 1rem;
          font-style: normal;
          border-left: 5px solid #d90a2c;
          padding-left: 25px;
          padding-right: 15px;
          line-height: 1.5;
        }
        
        /* 
        
        Breadcrumbs
        
        */
        
        .uk-breadcrumb span, .uk-breadcrumb a {
          color: #17161a;
        }
        .uk-breadcrumb>:last-child>a:not([href]), .uk-breadcrumb>:last-child>span {
          color: #8a8a8a;
          font-weight: 500;
        }
        .uk-breadcrumb>:nth-child(n+2):not(.uk-first-column):before {
          color: #17161a;
          content: "\f054";
          font-family: 'Font Awesome 5 Free';
          font-style: normal;
          font-variant: normal;
          text-rendering: auto;
          -webkit-font-smoothing: antialiased;
          font-weight: 900;
          display: inline-block;
          font-size: .75rem;
          margin: 0 10px 0 6px;
        }
        
        /* 
        
        Widgets
        
        */
        
        .widget .uk-list a {
          color: #17161a;
          transition: .2s ease-in-out;
          transition-property: color;
        }
        .uk-light .widget .uk-list a {
          color: #fff;
        }
        .widget .uk-list a:hover, .widget .uk-list li.uk-active a {
          color: #d90a2c;
          text-decoration: none;
        }
        h3.widget-title {
          font-weight: 600;
          letter-spacing: 0;
          color: #6a696c;
        }
        
        /* 
        
        Pagination
        
        */
        
        .archive-pagination .uk-pagination>.uk-active>* {
          color: #d90a2c;
        }
        
        /* 
        
        Default page header: plain bg, big heading etc...
        
        */
        
        .default-page-header {
          margin-top: 140px!important;
        }
        .default-page-header .uk-text-meta,
        .project-cover .uk-text-meta,
        .page-cover .uk-text-meta,
        .project-content .uk-text-meta {
          font-size:17px;
          height: 100%;
          line-height: 1.65;
          color: #17161a; 
        }
        .uk-light.default-page-header .uk-text-meta, 
        .uk-light.project-cover .uk-text-meta,
        .uk-light .default-page-header .uk-text-meta, 
        .uk-light .project-cover .uk-text-meta,
        .uk-light .page-cover .uk-text-meta,
        .uk-light.page-cover .uk-text-meta,
        .uk-light .project-content .uk-text-meta,
        .uk-light.project-content .uk-text-meta {
          color: #fff!important; 
        }
        
        
        /* 
        
        Responsive Devices
        
        */
        
        /* Large Desktops */
        @media only screen and (min-width: 1200px) {
          
          .uk-heading-medium {
            font-size: 4.5rem;
          }
          .uk-heading-large {
            font-size: 5rem;
          }
          
          #FooterTop .uk-grid-column-large, #FooterTop .uk-grid-large {
            margin-left: -220px;
          }
          #FooterTop .uk-grid-column-large>*, #FooterTop .uk-grid-large>* {
            padding-left: 220px;
          }
          
        }
        
        /* Desktops & tablets landscape*/
        @media only screen and (max-width: 1199px) {
          
          .uk-heading-large {
            font-size: 4rem;
          }
          
        }
        
        /* Tablets portrait*/
        @media only screen and (min-device-width : 960px) and (max-device-width : 1199px) {
          
        }
        @media only screen and (max-width: 959px) {
          
          .uk-heading-large {
            font-size: 4rem;
          }
          
        }
        
        /* Phones landscape */
        @media only screen and (min-device-width : 768px) and (max-device-width : 959px) {
          
        }
        @media only screen and (min-width: 768px) {
          
          #CTA {
            margin-top: -90px;
          }
          #CTA .uk-background-cover {
            margin: 100px 0;
          }
          
        }
        @media only screen and (max-width: 767px) {
          
          .uk-h1, h1 {
            font-size: 1.9rem;
          }
          .uk-h2, h2 {
            font-size: 1.5rem;
          }
          
          #Work .work-item img {
            max-height: 250px;
            width: 100%;
            object-fit: cover;
          }
          
          #CTA .uk-background-cover.uk-height-large {
            height: 150px;
          }
          #CTA .uk-accordion-title {
            font-size: 1rem;
          }
          #CTA .uk-padding-large {
            padding: 20px;
          }
          
          #Services .services-item img{
            max-height: 160px;
            width: 100%;
            object-fit: cover;
          }
          #Services .uk-grid {
            margin-top: -115px;
          }
          #Services *+.uk-grid-margin, #Services .uk-grid+.uk-grid, #Services .uk-grid>.uk-grid-margin {
              margin-top: 10px;
          }
          
          #PersonalLeader .leader-height {
            min-height: calc(70vh);
          }
          #PersonalLeader .leader-heading {
            font-size: 2.7rem;
          }
          
          #FooterTop .uk-grid-column-small>*, #FooterTop .uk-grid-small>* {
            padding-left: 15px;
          }
          #FooterTop *+.uk-grid-margin-medium, #FooterTop .uk-grid+.uk-grid-medium, #FooterTop .uk-grid+.uk-grid-row-medium, #FooterTop .uk-grid-medium>.uk-grid-margin, #FooterTop .uk-grid-row-medium>.uk-grid-margin {
            margin-top: 10px;
          }
