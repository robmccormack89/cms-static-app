    {% endblock %}

    <main id="MainContent" role="main">
      {% block content %}
        Sorry, Nothing to Display! Content goes here...
      {% endblock %}
    </main>

    {% block footer_top %}
      <section id="FooterTop" class="uk-section uk-background-default">
        <div class="uk-container">
          <div class="uk-grid-large" uk-grid>
            <div class="uk-width-2-3@m">
              <p class="uk-text-meta uk-text-uppercase uk-text-bold uk-margin-small-bottom">Say Hello</p>
              <h2 class="uk-h1 uk-margin-remove">Request a free quote</h2>
              {% if form_display_msg is empty %}
                <form method="post" class="uk-grid-column-small uk-grid-row-medium uk-margin-medium-top uk-form-stacked" uk-grid>
                  <div class="uk-width-1-2">
                    <label class="uk-form-label uk-text-bold" for="name">Name (required)</label>
                    <div class="uk-form-controls">
                      <input class="uk-input uk-border-rounded uk-form-large" name="name" id="name" type="text" placeholder="Your name..">
                    </div>
                  </div>
                  <div class="uk-width-1-2">
                    <label class="uk-form-label uk-text-bold" for="email">Email (required)</label>
                    <div class="uk-form-controls">
                      <input class="uk-input uk-border-rounded uk-form-large" name="email" id="email" type="email" placeholder="Your email address..">
                    </div>
                  </div>
                  <div class="uk-width-1-2">
                    <label class="uk-form-label uk-text-bold" for="phone">Phone (optional)</label>
                    <div class="uk-form-controls">
                      <input class="uk-input uk-border-rounded uk-form-large" name="phone" id="phone" type="tel" placeholder="Your phone number..">
                    </div>
                  </div>
                  <div class="uk-width-1-2">
                    <label class="uk-form-label uk-text-bold" for="subject">Subject (optional)</label>
                    <div class="uk-form-controls">
                      <input class="uk-input uk-border-rounded uk-form-large" name="subject" id="subject" type="text" maxlength="255" placeholder="What your message is about..">
                    </div>
                  </div>
                  <div class="uk-width-1-2">
                    <label class="uk-form-label uk-text-bold" for="company">Company (optional)</label>
                    <div class="uk-form-controls">
                      <input class="uk-input uk-border-rounded uk-form-large" name="company" id="company" type="text" placeholder="Your company name..">
                    </div>
                  </div>
                  <div class="uk-width-1-2">
                    <label class="uk-form-label uk-text-bold" for="budget">Choose a Budget (optional)</label>
                    <div class="uk-form-controls">
                      <select class="uk-select uk-border-rounded uk-form-large" name="budget" id="budget">
                        <option value="under-5k">Less than €5,000</option>
                        <option value="5-10k">€5,000-€10,000</option>
                        <option value="over-10k">Over €10,000</option>
                        <option value="not-applicable">Not applicable</option>
                      </select>
                    </div>
                  </div>
                  <div>
                    <input type="submit" value="Submit" class="uk-button uk-button-primary uk-button-small uk-button-secondary uk-border-rounded">
                  </div>
                </form>
              {% else %}
                <p>{{form_display_msg}}</p>
              {% endif %}
            </div>
            <div class="uk-width-1-3@m">
              <p class="uk-text-meta uk-text-uppercase uk-text-bold uk-margin-small-bottom">Get in Touch</p>
              <h2 class="uk-h1 uk-margin-remove">Email me</h2>
              <p>
                For project inquiries only:<br>
                <a class="uk-button uk-button-text uk-text-lowercase">info@robertmccormack.com</a>
              </p>
              <p class="uk-margin-medium-top">
                For other questions:<br>
                <a class="uk-button uk-button-text uk-text-lowercase">info@robertmccormack.com</a>
              </p>
            </div>
          </div>
        </div>
      </section>
    {% endblock %}
    
    {% block footer %}
      <footer id="Footer" class="default-footer home-footer uk-section uk-section-small uk-background-default">
        {% include 'footer-main.twig' %}
      </footer>
    {% endblock %}
    
    {% block base_scripts %}
      <script type='text/javascript' src='/public/js/global/global.min.js'></script>
      <script type='text/javascript' src='/public/js/theme.js'></script>
